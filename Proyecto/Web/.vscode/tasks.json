{
	"version": "2.0.0",
	"tasks": [
	  {
		"label": "build",
		"command": "dotnet",
		"type": "process",
		"args": [
		  "build",
		  "${workspaceFolder}/Presentacion/Presentacion.csproj"
		],
		"problemMatcher": "$msCompile"
	  },
	  {
		"label": "publish",
		"command": "dotnet",
		"type": "process",
		"args": [
		  "publish",
		  "${workspaceFolder}/Presentacion/Presentacion.csproj",
		  "--output",
		  "${workspaceFolder}/bin/Debug/net7.0/publish"
		],
		"problemMatcher": "$msCompile"
	  }
	]
  }
  