{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "IIS Express",
        "type": "coreclr",
        "request": "launch",
        "preLaunchTask": "build",
        "program": "${workspaceFolder}/RoyalShoes/bin/Debug/net6.0/RoyalShoes.dll",
        "args": [],
        "cwd": "${workspaceFolder}",
        "stopAtEntry": false,
        "launchBrowser": {
          "enabled": true,
          "args": "${auto-detect-url}",
          "windows": {
            "command": "cmd.exe",
            "args": "/C start ${auto-detect-url}"
          },
          "osx": {
            "command": "open"
          },
          "linux": {
            "command": "xdg-open"
          }
        },
        "env": {
          "ASPNETCORE_ENVIRONMENT": "Development"
        },
        "sourceFileMap": {
          "/Views": "${workspaceFolder}/Views"
        }
      }
    ]
  }